{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
      "queues": {
        "maxPollingInterval": "00:00:10",
        "visibilityTimeout": "00:00:30",
        "maxDequeueCount": 5,
        "batchSize": 16,
        "newBatchThreshold": 8,
        "messageEncoding": "base64"
      }
    }
}